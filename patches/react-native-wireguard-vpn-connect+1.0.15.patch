+package com.wireguardvpn
+
+import android.app.Notification
+import android.app.NotificationChannel
+import android.app.NotificationManager
+import android.app.PendingIntent
+import android.content.Context
+import android.content.Intent
+import android.net.VpnService
+import android.os.Build
+import android.os.Handler
+import android.os.Looper
+import androidx.core.app.NotificationCompat
+import com.wireguard.android.backend.Statistics
+import java.text.DecimalFormat
+
+class WireGuardVpnService : VpnService() {
+    companion object {
+        private const val NOTIFICATION_ID = 1001
+        private const val CHANNEL_ID = "wireguard_vpn_channel"
+        private const val CHANNEL_NAME = "WireGuard VPN"
+
+        private var isRunning = false
+        private var currentStats: Statistics? = null
+
+        fun isServiceRunning(): Boolean = isRunning
+
+        fun updateStats(stats: Statistics?) {
+            currentStats = stats
+        }
+    }
+
+    private val handler = Handler(Looper.getMainLooper())
+    private var updateRunnable: Runnable? = null
+    private var lastRxBytes = 0L
+    private var lastTxBytes = 0L
+    private var lastUpdateTime = 0L
+
+    override fun onCreate() {
+        super.onCreate()
+        println("WireGuardVpnService onCreate() called")
+        isRunning = true
+        try {
+            createNotificationChannel()
+            println("Notification channel created")
+
+            val notification = createNotification()
+            println("Notification created: ${notification.extras}")
+
+            startForeground(NOTIFICATION_ID, notification)
+            println("Foreground service started with notification ID: $NOTIFICATION_ID")
+
+            startStatsUpdater()
+            println("Stats updater started")
+        } catch (e: Exception) {
+            println("Error in onCreate: ${e.message}")
+            e.printStackTrace()
+        }
+    }
+
+    override fun onStartCommand(intent: Intent?, flags: Int, startId: Int): Int {
+        println("WireGuardVpnService onStartCommand() called")
+        // Ensure notification is shown
+        try {
+            val notification = createNotification()
+            val notificationManager = getSystemService(Context.NOTIFICATION_SERVICE) as NotificationManager
+            notificationManager.notify(NOTIFICATION_ID, notification)
+            println("Notification updated in onStartCommand")
+        } catch (e: Exception) {
+            println("Error updating notification in onStartCommand: ${e.message}")
+            e.printStackTrace()
+        }
+        return START_STICKY
+    }
+
+    override fun onDestroy() {
+        isRunning = false
+        stopStatsUpdater()
+        super.onDestroy()
+    }
+
+    private fun createNotificationChannel() {
+        if (Build.VERSION.SDK_INT >= Build.VERSION_CODES.O) {
+            try {
+                val channel = NotificationChannel(
+                    CHANNEL_ID,
+                    CHANNEL_NAME,
+                    NotificationManager.IMPORTANCE_LOW
+                ).apply {
+                    description = "Shows VPN connection status and network speed"
+                    setShowBadge(false)
+                    enableVibration(false)
+                    enableLights(false)
+                }
+
+                val notificationManager = getSystemService(NotificationManager::class.java)
+                notificationManager?.createNotificationChannel(channel)
+                println("Notification channel '$CHANNEL_ID' created successfully")
+            } catch (e: Exception) {
+                println("Error creating notification channel: ${e.message}")
+                e.printStackTrace()
+            }
+        } else {
+            println("Android version < O, no channel needed")
+        }
+    }
+
+    private fun createNotification(): Notification {
+        // Get main app intent
+        val packageName = applicationContext.packageName
+        val intent = applicationContext.packageManager.getLaunchIntentForPackage(packageName)
+        val pendingIntent = PendingIntent.getActivity(
+            this,
+            0,
+            intent,
+            PendingIntent.FLAG_IMMUTABLE or PendingIntent.FLAG_UPDATE_CURRENT
+        )
+
+        // Calculate network speed
+        val (downloadSpeed, uploadSpeed) = calculateSpeed()
+
+        // Format speeds
+        val downloadText = formatSpeed(downloadSpeed)
+        val uploadText = formatSpeed(uploadSpeed)
+
+        return NotificationCompat.Builder(this, CHANNEL_ID)
+            .setContentTitle("DeVPN Connected")
+            .setContentText(" $downloadText   $uploadText")
+            .setSmallIcon(android.R.drawable.stat_sys_upload_done) // Using system icon
+            .setOngoing(true)
+            .setContentIntent(pendingIntent)
+            .setPriority(NotificationCompat.PRIORITY_LOW)
+            .setCategory(NotificationCompat.CATEGORY_SERVICE)
+            .setShowWhen(false)
+            .setVisibility(NotificationCompat.VISIBILITY_PUBLIC)
+            .build()
+    }
+
+    private fun calculateSpeed(): Pair<Double, Double> {
+        val stats = currentStats ?: return Pair(0.0, 0.0)
+
+        val currentTime = System.currentTimeMillis()
+        val timeDiff = (currentTime - lastUpdateTime) / 1000.0 // seconds
+
+        if (timeDiff <= 0 || lastUpdateTime == 0L) {
+            lastUpdateTime = currentTime
+            return Pair(0.0, 0.0)
+        }
+
+        // Get total bytes from all peers
+        var totalRxBytes = 0L
+        var totalTxBytes = 0L
+
+        try {
+            // Try to access peers data using reflection
+            val peersData = stats.peers()
+            if (peersData != null) {
+                // Use reflection to safely access Map entries
+                val mapClass = peersData.javaClass
+                val entrySetMethod = mapClass.getMethod("entrySet")
+                val entrySet = entrySetMethod.invoke(peersData) as? Set<*>
+
+                entrySet?.forEach { entry ->
+                    try {
+                        // Get value from Map.Entry
+                        val getValueMethod = entry?.javaClass?.getMethod("getValue")
+                        val peerStats = getValueMethod?.invoke(entry)
+
+                        if (peerStats != null) {
+                            // Try to get rxBytes and txBytes using reflection
+                            val rxBytesField = peerStats.javaClass.getDeclaredField("rxBytes")
+                            val txBytesField = peerStats.javaClass.getDeclaredField("txBytes")
+
+                            rxBytesField.isAccessible = true
+                            txBytesField.isAccessible = true
+
+                            val rx = rxBytesField.get(peerStats) as? Long ?: 0L
+                            val tx = txBytesField.get(peerStats) as? Long ?: 0L
+
+                            totalRxBytes += rx
+                            totalTxBytes += tx
+                        }
+                    } catch (e: Exception) {
+                        // Skip this peer if we can't get stats
+                    }
+                }
+            }
+        } catch (e: Exception) {
+            // If we can't get stats, return 0
+            // This is expected if Statistics API is not available
+            return Pair(0.0, 0.0)
+        }
+
+        // Calculate speed in bytes/sec
+        val downloadSpeed = if (timeDiff > 0) (totalRxBytes - lastRxBytes) / timeDiff else 0.0
+        val uploadSpeed = if (timeDiff > 0) (totalTxBytes - lastTxBytes) / timeDiff else 0.0
+
+        // Update last values
+        lastRxBytes = totalRxBytes
+        lastTxBytes = totalTxBytes
+        lastUpdateTime = currentTime
+
+        return Pair(downloadSpeed.coerceAtLeast(0.0), uploadSpeed.coerceAtLeast(0.0))
+    }
+
+    private fun formatSpeed(bytesPerSecond: Double): String {
+        val df = DecimalFormat("#.##")
+        return when {
+            bytesPerSecond < 0 -> "0 B/s"
+            bytesPerSecond < 1024 -> "${df.format(bytesPerSecond)} B/s"
+            bytesPerSecond < 1024 * 1024 -> "${df.format(bytesPerSecond / 1024)} KB/s"
+            bytesPerSecond < 1024 * 1024 * 1024 -> "${df.format(bytesPerSecond / (1024 * 1024))} MB/s"
+            else -> "${df.format(bytesPerSecond / (1024 * 1024 * 1024))} GB/s"
+        }
+    }
+
+    private fun startStatsUpdater() {
+        updateRunnable = object : Runnable {
+            override fun run() {
+                updateNotification()
+                handler.postDelayed(this, 2000) // Update every 2 seconds
+            }
+        }
+        handler.post(updateRunnable!!)
+    }
+
+    private fun stopStatsUpdater() {
+        updateRunnable?.let { handler.removeCallbacks(it) }
+        updateRunnable = null
